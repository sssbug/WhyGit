@using Why.Controllers


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Why </title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Why_Did_S.I.H._Die.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/Detail.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/prem.css" asp-append-version="true" />
     
</head>
<body>
    <header class="navable">
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light  mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Why_Did_S.I.H._Die</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-center">
                    <ul class="navbar-nav">

                        <li class="nav-item-chose">
                            <a class=" choseButton" asp-area="" asp-controller="Home" asp-action="Index" asp-route-name="Suicide">Suicide</a>
                        </li>
                        <li class="nav-item-chose">
                            <a class=" choseButton" asp-area="" asp-controller="Home" asp-action="Index" asp-route-name="Natural">Natural</a>
                        </li>
                        <li class="nav-item-chose">
                            <a class=" choseButton" asp-area="" asp-controller="Home" asp-action="Index" asp-route-name="Accidental">Accidental</a>
                        </li>
                        <li class="nav-item-chose">
                            <a class=" choseButton" asp-area="" asp-controller="Home" asp-action="Index" asp-route-name="Homicide">Homicide</a>
                        </li>
                        <li class="nav-item-chose">
                            <a class=" choseButton" asp-area="" asp-controller="Home" asp-action="Index" asp-route-name="Undetermined">Undetermined</a>
                        </li>
                        <li class="nav-item-chose">
                            <a class=" choseButton" asp-area="" asp-controller="Home" asp-action="Index" asp-route-name="Pending">Pending</a>
                        </li>
                        <li class="nav-item-chose">
                            <a class=" choseButton" asp-area="" asp-controller="Home" asp-action="Index" asp-route-name="">All</a>
                        </li>
                    </ul>
                </div>
                <div class="justify-content-end">
                    <ul class="navbar-nav ">

                        @if (ViewBag.userCount != null)
                        {

                            <li class="nav-item">
                                <a class="nav-link text-dark" asp-area="" asp-controller="Admin" asp-action="Index">Admin</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link text-white btn btn-primary" asp-area="" asp-controller="Login" asp-action="Logout">Logout</a>
                            </li>

                        }
                        else
                        {
                            
                            <li class="nav-item ">
                                <a class="login" asp-area="" asp-controller="Login" asp-action="Login">Sign in</a>
                            </li>
                            <li class="nav-item ">
                                <a class="signup" asp-controller="Account" asp-action="Register">Sign Up</a>
                            </li>
                        }
                    </ul>
                </div>
            </div>
        </nav>
    </header>

    @RenderBody()



    <footer class="footer text-muted">

        <div class="privacy">
            &copy; 2022 - Why_Did_S.I.H._Die - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false);
</body>
</html>
