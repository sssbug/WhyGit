@using Why.Data.Models
@model List<Thumb>
@{
    ViewData["Title"] = "Home Page";
    
}

<div class="grid-base">

    <div class="grid-container">
        @foreach (var item in Model)
        {
            
            <a class="box" asp-controller="Home" asp-action="Detail" asp-route-id="@item.ThumbId">
                <div class="box" style="">
                    <h5 >@item.ThumbName </h5>
                </div>
            </a>

        }
    </div>

</div>